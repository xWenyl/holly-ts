{
  "name": "holly-ts",
  "version": "1.0.0",
  "description": "Discord bot written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/index.ts",
    "start": "node .",
    "build": "tsc",
    "deploy": "cross-env IS_SCRIPT=true ts-node src/scripts/deploy",
    "deploy-prod": "cross-env NODE_ENV=production npm run deploy"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "author": "Wenyl",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3"
  }
}
